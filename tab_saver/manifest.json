{
  "manifest_version": 2,
  "name": "Tab Saver",
  "version": "1.0",
  "description": "A Chrome extension to capture and save open tabs.",
  "permissions": ["tabs", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
